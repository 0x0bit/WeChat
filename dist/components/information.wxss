@font-face{font-family:'iconfont';src:url(data:font/truetype;charset=utf-8;base64,) format('truetype');font-weight:normal;font-style:normal}.information{display:none;position:fixed;z-index:2;bottom:0;width:100%;margin:0 auto;height:1000rpx;opacity:1;overflow:hidden;background-color:#fff;font-size:14px;line-height:18px;word-break:break-all;will-change:transform;transform:translate3d(0, 550rpx, 0);animation:slideInUpHalf .3s;transition:transform .3s}.information.information-show{display:block}.information .information-content{height:200rpx;width:initial}.information .information-content .scroll-container{padding:20px}.information.isDetail{transform:initial}.information.isDetail .information-content{height:750rpx}.information .word{font-size:16pt;font-weight:400;color:#209e85}.information .pron{color:gray;margin-left:10rpx}.information .audio{font-family:'iconfont';float:right;margin-top:22rpx;color:#209e85}.information .audio:before{content:'\e605';margin-right:10rpx;font-size:16pt}.information .definition{margin-top:16rpx}.information .definition .definition-item{display:block}.information .detail-btn{color:#28bea0;border:1px solid #28bea0;background-color:#fff;width:300rpx;border-radius:5px;padding-top:2px;font-size:14px;overflow:hidden;height:40px;position:absolute;left:50%;top:300rpx;transform:translateX(-50%)}.information .detail-btn.close{top:850rpx}.information .examples{color:gray;margin:40rpx 0 20rpx}.information .example-item{margin-bottom:30rpx;word-break:keep-all}.information .example-item .current-word{color:#209e85}.information .example-item .example-translation{margin-top:5rpx;word-break:break-all}.slidedown-half{animation:slideOutDownHalf .3s}.sliderdown{animation:slideOutDown .3s}.slidebar{width:100%;background-color:#eee;height:70rpx;color:#bdbdbd;text-align:center;line-height:70rpx;font-family:'iconfont'}.slidebar:before{content:'\e6e0';margin-right:10rpx;font-size:16pt}@keyframes slideInUpHalf{from{transform:translate3d(0, 100%, 0);visibility:visible}to{transform:translate3d(0, 550rpx, 0)}}@keyframes slideOutDownHalf{from{transform:translate3d(0, 550rpx, 0)}to{visibility:hidden;transform:translate3d(0, 100%, 0)}}@keyframes slideInUp{from{transform:translate3d(0, 100%, 0);visibility:visible}to{transform:translate3d(0, 0, 0)}}@keyframes slideOutDown{from{transform:translate3d(0, 0, 0)}to{visibility:hidden;transform:translate3d(0, 100%, 0)}}